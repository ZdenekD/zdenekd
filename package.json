{
  "author": "ZdenekD",
  "name": "zdenekd",
  "version": "1.0.0",
  "description": "Web",
  "license": "MIT",
  "homepage": "git@github.com:ZdenekD/zdenekd.git#readme",
  "repository": "git@github.com:ZdenekD/zdenekd.git",
  "scripts": {
    "reinstall": "rm -rf 'yarn.lock' 'node_modules' && yarn",
    "debug": "NODE_OPTIONS='--inspect' next dev -p 8000",
    "dev": "next dev -p 8000",
    "start": "next start -p 8000",
    "build": "next build",
    "story": "start-storybook -p 8020",
    "story:build": "build-storybook -c .storybook -o .storybook/build",
    "chromatic": "chromatic",
    "test": "yarn test:js",
    "test:js": "jest --watch",
    "test:js:verbose": "jest --watch --verbose",
    "test:js:coverage": "jest --coverage",
    "test:js:staged": "jest --bail --findRelatedTests --passWithNoTests",
    "test:js:clear": "jest --clearCache",
    "test:playwright": "playwright test",
    "test:playwright:report": "playwright show-report __test__/playwright/report",
    "lint": "yarn lint:js && yarn lint:css && yarn prettier",
    "lint:js": "eslint --ext .ts,.tsx ./src --cache --fix",
    "lint:css": "stylelint ./src/**/*.css --cache --fix --color",
    "typecheck": "tsc -b",
    "prettier": "prettier --write ./ --cache --check",
    "prepare": "husky install",
    "update": "yarn upgrade-interactive"
  },
  "packageManager": "yarn@3.3.0",
  "engines": {
    "node": ">=16.15.0"
  },
  "msw": {
    "workerDirectory": "./public"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint:js",
      "yarn test:js:staged",
      "bash -c 'yarn typecheck'"
    ],
    "*.css": "yarn lint:css",
    "*.{json,yml,rc,html}": "yarn prettier"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "env": {
      "test": {
        "plugins": [
          "transform-dynamic-import"
        ]
      }
    }
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@next/eslint-plugin-next": "^13.2.4",
    "@playwright/test": "^1.32.1",
    "@storybook/addon-a11y": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storysource": "^6.5.16",
    "@storybook/addons": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/client-api": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "6.4.22",
    "@svgr/webpack": "^7.0.0",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/animejs": "^3.1.7",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.192",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "babel-jest": "^29.5.0",
    "babel-plugin-transform-dynamic-import": "^2.1.0",
    "chromatic": "^6.17.2",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.37.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-playwright": "^0.12.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-canvas-mock": "^2.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-next-dynamic": "^1.0.1",
    "jest-serializer-html": "^7.1.0",
    "lint-staged": "^13.2.0",
    "msw": "^1.2.1",
    "msw-storybook-addon": "^1.8.0",
    "postcss": "^8.4.21",
    "postcss-custom-media": "^9.1.2",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^8.3.0",
    "postcss-reporter": "^7.0.5",
    "prettier": "^2.8.7",
    "react-docgen-typescript-plugin": "^1.0.5",
    "storybook-addon-apollo-client": "^4.1.4",
    "storybook-addon-next-router": "^4.0.2",
    "style-loader": "^3.3.2",
    "stylelint": "^15.4.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-recommended": "^11.0.0",
    "stylelint-csstree-validator": "^2.1.0",
    "stylelint-order": "^6.0.3",
    "webpack": "^5.77.0"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.11",
    "@lottiefiles/react-lottie-player": "^3.5.3",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/nextjs": "^7.46.0",
    "@use-it/event-listener": "^0.1.7",
    "animejs": "^3.2.1",
    "framer-motion": "^7.10.3",
    "lodash": "^4.17.21",
    "next": "^13.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "swr": "^2.1.1",
    "typescript": "^5.0.3",
    "yup": "^0.32.11",
    "zustand": "^4.3.7"
  }
}
