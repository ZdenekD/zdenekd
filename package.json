{
  "author": "ZdenekD",
  "name": "zdenekd",
  "version": "1.0.0",
  "description": "Web",
  "license": "MIT",
  "homepage": "git@github.com:zdenekd/zdenekd.git#readme",
  "repository": "git@github.com:zdenekd/zdenekd.git",
  "scripts": {
    "reinstall": "rm -rf 'yarn.lock' 'node_modules' && yarn",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "dev": "next dev -p 8000",
    "start": "next start -p 8000",
    "build": "next build",
    "analyze": "cross-env ANALYZE=true next build",
    "jest": "jest --watch",
    "jest:verbose": "jest --watch --verbose",
    "jest:coverage": "jest --coverage",
    "jest:staged": "jest --bail --findRelatedTests",
    "story": "start-storybook -p 8020",
    "story:build": "build-storybook -c .storybook -o .storybook/build",
    "chromatic": "chromatic",
    "cypress": "cypress run -p 8010 -q",
    "cypress:open": "cypress open -p 8010",
    "hint:local": "hint http://localhost:8000",
    "hint:dev": "hint https://zdenekd-ditezdenek.vercel.app",
    "hint:prod": "hint https://zdenekd.cz",
    "lint": "yarn lint:js && yarn lint:css && yarn typecheck",
    "lint:js": "eslint --ext .ts,.tsx ./src --cache --fix",
    "lint:css": "stylelint ./src/**/*.css --cache --fix --color",
    "typecheck": "tsc -b",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=14.17.0",
    "npm": ">=6.14.13"
  },
  "msw": {
    "workerDirectory": "__test__"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint:js",
      "yarn jest:staged",
      "bash -c 'yarn typecheck'"
    ],
    "*.css": "yarn lint:css",
    "*.{json,md,yml,html}": "prettier --write"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@hint/configuration-web-recommended": "^8.1.0",
    "@next/bundle-analyzer": "^12.1.0",
    "@storybook/addon-a11y": "^6.1.18",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storysource": "^6.1.18",
    "@storybook/addons": "^6.1.18",
    "@storybook/builder-webpack5": "^6.5.0-alpha.40",
    "@storybook/client-api": "^6.3.6",
    "@storybook/manager-webpack5": "^6.5.0-alpha.40",
    "@storybook/react": "^6.1.18",
    "@svgr/webpack": "^6.2.1",
    "@types/animejs": "^3.1.2",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^27.0.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/underscore": "^1.10.24",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.0",
    "babel-jest": "^27.0.2",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "chromatic": "^6.0.4",
    "cssnano": "^5.0.2",
    "cypress": "^9.5.0",
    "dotenv": "^16.0.0",
    "enzyme": "^3.11.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^4.0.8",
    "hint": "^6.0.3",
    "husky": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.4",
    "jest-canvas-mock": "^2.2.0",
    "jest-serializer-html": "^7.0.0",
    "lint-staged": "^12.3.4",
    "msw": "0.38.2",
    "msw-storybook-addon": "^1.6.0",
    "postcss": "^8.2.6",
    "postcss-custom-media": "^8.0.0",
    "postcss-import": "^14.0.0",
    "postcss-nested": "^5.0.3",
    "postcss-preset-env": "^7.4.1",
    "postcss-reporter": "^7.0.2",
    "prettier": "^2.2.1",
    "storybook-addon-next-router": "^3.0.7",
    "style-loader": "^3.2.1",
    "stylelint": "^14.0.1",
    "stylelint-a11y": "^1.2.2",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-csstree-validator": "^2.0.0",
    "stylelint-order": "^5.0.0",
    "typescript-plugin-css-modules": "^3.4.0",
    "webpack": "^5.48.0"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.8.3",
    "@lottiefiles/react-lottie-player": "^3.0.0",
    "@sendgrid/mail": "^7.4.2",
    "@sentry/nextjs": "^6.7.1",
    "@use-it/event-listener": "^0.1.6",
    "animejs": "^3.2.1",
    "framer-motion": "^6.2.6",
    "next": "^12.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^7.19.1",
    "typescript": "^4.3.5",
    "underscore": "^1.12.0",
    "yup": "^0.32.11"
  }
}
