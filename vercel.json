{
  "name": "ZdenekD",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains; preload"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "origin, x-requested-with, content-type"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET"
        },
        {
          "key": "Cache-Control",
          "value": "private, max-age=180"
        }
      ]
    },
    {
      "source": "(.*).woff2|(.*).woff|(.*).gif|(.*).jpg|(.*).jpeg|(.*).mp4|(.*).png|(.*).svg|(.*).webp|(.*).webm|(.*).css|(.*).json|(.*).ico|(.*).webmanifest",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "(.*).js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "private, max-age=31536000, immutable"
        },
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    },
    {
      "source": "(.*).ico",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/x-icon"
        }
      ]
    },
    {
      "source": "(.*)manifest.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/manifest+json; charset=utf-8"
        }
      ]
    }
  ],
  "build": {
    "env": {
      "EMAIL_RECIPIENT": "@email-recipient-zdenekd",
      "SENDGRID_API_KEY": "@sendgrid-api-key-zdenekd",
      "SENDGRID_TEMPLATE_ID": "@sendgrid-template-id-zdenekd"
    }
  }
}
